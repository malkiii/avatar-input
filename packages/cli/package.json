{
  "name": "react-image-form-input",
  "description": "A simple unstyled input component to select avatars and crop images.",
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "Malki Abderrahmane",
    "url": "https://malki.me"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "homepage": "https://malkiii.github.io/react-image-form-input",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/malkiii/react-image-form-input",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "format:check": "prettier --check \"**/*.{ts,tsx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx}\" --cache",
    "publish": "pnpm build && pnpm publish --access public"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.24",
    "@types/eslint": "^8.56.5",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": []
}

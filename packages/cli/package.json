{
  "name": "react-image-form-input",
  "description": "CLI to add a simple unstyled input component to select avatars and crop images using.",
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "Malki Abderrahmane",
    "url": "https://malki.me"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "homepage": "https://malkiii.github.io/react-image-form-input",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/malkiii/react-image-form-input",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "format:check": "prettier --check \"**/*.{ts,tsx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx}\" --cache",
    "publish": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-typescript": "^7.24.4",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.11.24",
    "type-fest": "^4.15.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "keywords": []
}

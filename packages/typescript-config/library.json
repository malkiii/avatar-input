{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "composite": false,
    "skipLibCheck": true,
    "inlineSources": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "module": "ESNext"
  }
}
